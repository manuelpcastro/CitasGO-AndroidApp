<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ugr.citasgo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Presentadores.Registro"></activity>
        <activity android:name=".Presentadores.Cancelacion" />
        <activity android:name=".Presentadores.MisCitas" />
        <activity android:name=".Presentadores.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Presentadores.Reserva" />
        <activity android:name=".Presentadores.Operaciones"/>
        <activity android:name=".Presentadores.Calendario" />
        <activity android:name=".Presentadores.Dia" />
    </application>

</manifest>